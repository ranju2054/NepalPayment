@model IEnumerable<NepalPayment.Table>

@{
    ViewBag.Title = "Index";
}

<h2>Task Lists</h2>

<p>
    @Html.ActionLink("Add Task ", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TaskList)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssignedOn)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TaskList)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AssignedOn)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
